{
  "name": "braindump",
  "version": "1.0.0",
  "description": "This braindump is generated via [ox-hugo](https://github.com/kaushalmodi/ox-hugo) and uses an adapted version of the [er](https://github.com/lingxz/er) theme.",
  "main": "index.js",
  "scripts": {
    "build": "npm run lunr:index && hugo",
    "lunr:index": "mkdir -p static && node lib/hli.js -i 'content/**' -o 'static/lunr.json' -l 'toml' -d '+++'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^9.7.6",
    "cssnano": "^4.1.10",
    "glob": "^7.1.6",
    "gray-matter": "^4.0.2",
    "lunr": "^2.3.8",
    "postcss-cli": "^7.1.0",
    "remove-markdown": "^0.3.0",
    "striptags": "^3.1.1",
    "tailwindcss": "^1.2.0",
    "toml": "^3.0.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.2"
  }
}
